    "cors": "^2.8.5",
    "dayjs": "^1.11.5",
    "dotenv": "^16.0.1",
    "eslint": "^8.23.0",
    "express": "^4.18.1",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.8.0",
    "pino": "^8.4.2",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.8"
  }
